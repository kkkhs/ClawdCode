{
  "default": {
    "apiKey": "your-api-key-here",
    "baseURL": "https://api.openai.com/v1",
    "model": "gpt-4"
  },
  "ui": {
    "theme": "dark"
  },
  "permissions": {
    "allow": [
      "Read(**/*)",
      "Glob(**/*)",
      "Grep(**/*)",
      "Bash(npm:*)",
      "Bash(git:*)",
      "Bash(bun:*)"
    ],
    "deny": [
      "Bash(rm -rf:*)",
      "Bash(sudo:*)",
      "Write(/etc/*)",
      "Write(/usr/*)"
    ],
    "ask": []
  },
  "defaultPermissionMode": "default",
  "mcpEnabled": true,
  "mcpServers": {
    "filesystem": {
      "type": "stdio",
      "command": "mcp-server-filesystem",
      "args": ["./"]
    }
  }
}
